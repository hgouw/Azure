    format      ( bmp | doc | gif | htm | jpg | pdf |
                  ps | tif )                                #REQUIRED
    inline      ( y | n )                                   "n"
    use         ( all | print | online | cd | printonline |
                  printcd | onlinecd )                      "all"
    halign      ( left | center | right )                   "center"
    height      CDATA                                       #IMPLIED
    rotate      CDATA                                       #IMPLIED
    valign      ( top | middle | bottom )                   "middle"
    width       CDATA                                       #IMPLIED           >


<!-- ======================================================================= -->
